{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/made-in-canada/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/made-in-canada/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/made-in-canada",
  "projects": {
    "electrodb": {
      "type": "library",
      "root": "libs/electrodb",
      "entryFile": "index",
      "sourceRoot": "libs/electrodb/src",
      "compilerOptions": {
        "tsConfigPath": "libs/electrodb/tsconfig.lib.json"
      }
    },
    "made-in-canada": {
      "type": "application",
      "root": "apps/made-in-canada",
      "entryFile": "main",
      "sourceRoot": "apps/made-in-canada/src",
      "compilerOptions": {
        "tsConfigPath": "apps/made-in-canada/tsconfig.app.json"
      }
    },
    "quickeat": {
      "type": "application",
      "root": "apps/quickeat",
      "entryFile": "main",
      "sourceRoot": "apps/quickeat/src",
      "compilerOptions": {
        "tsConfigPath": "apps/quickeat/tsconfig.app.json"
      }
    },
    "service-b": {
      "type": "application",
      "root": "apps/service-b",
      "entryFile": "main",
      "sourceRoot": "apps/service-b/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-b/tsconfig.app.json"
      }
    },
    "service-demo": {
      "type": "application",
      "root": "apps/service-demo",
      "entryFile": "main",
      "sourceRoot": "apps/service-demo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-demo/tsconfig.app.json"
      }
    },
    "todos": {
      "type": "application",
      "root": "apps/todos",
      "entryFile": "main",
      "sourceRoot": "apps/todos/src",
      "compilerOptions": {
        "tsConfigPath": "apps/todos/tsconfig.app.json"
      }
    },
    "todos-sls": {
      "type": "application",
      "root": "apps/todos",
      "entryFile": "main-sls",
      "sourceRoot": "apps/todos/src",
      "compilerOptions": {
        "tsConfigPath": "apps/todos/tsconfig.app.json"
      }
    }
  }
}