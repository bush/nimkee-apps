{
  "name": "made-in-canada",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/made-in-canada/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/made-in-canada/test/jest-e2e.json",
    "update-build": "echo \"BUILD_ID=$(date +%s)\" > ",
    "todos-sls:build": "npm run update-build apps/todos/.env.build.local && nest build todos-sls && cp apps/todos/.env.build.local serverless/todos/apps/todos",
    "test:todos-repos": "NODE_ENV=development jest apps/todos/src/todos/repositories/respository.spec.ts",
    "test:todos": "jest todos --testPathIgnorePatterns=apps/todos/src/todos/repositories/respository.spec.ts",
    "test:todos:e2e": "NODE_ENV=development jest --config ./apps/todos/test/jest-e2e.json todos",
    "service-demo-sls:build": "nest build service-demo-sls && nest build service-demo-sls-sqs",
    "test:service-demo": "jest --testPathPattern=\"libs/sns-sqs/src/server/sqs-server.spec|apps/service-demo/src/main-sls\""
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.712.0",
    "@aws-sdk/client-sns": "^3.995.0",
    "@aws-sdk/client-sqs": "^3.995.0",
    "@aws-sdk/credential-providers": "^3.758.0",
    "@codegenie/serverless-express": "^4.16.0",
    "@grpc/grpc-js": "^1.13.4",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/swagger": "^11.1.6",
    "@nestjs/websockets": "^11.1.6",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1692.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "copy-webpack-plugin": "^13.0.0",
    "dockerode": "^4.0.8",
    "dockerode-compose": "^1.4.0",
    "electrodb": "^3.0.1",
    "i": "^0.3.7",
    "jwks-rsa": "^3.1.0",
    "npm": "^11.6.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "terser-webpack-plugin": "^5.3.10",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.1.6",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/dockerode": "^3.3.44",
    "@types/dockerode-compose": "^1.4.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/shelljs": "^0.8.17",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "serverless-offline": "^14.4.0",
    "shelljs": "^0.10.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/electrodb(|/.*)$": "<rootDir>/libs/electrodb/src/$1",
      "^@app/service-bus(|/.*)$": "<rootDir>/libs/service-bus/src/$1",
      "^@app/sns-sqs(|/.*)$": "<rootDir>/libs/sns-sqs/src/$1"
    }
  }
}